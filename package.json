{
  "name": "isol8",
  "version": "0.13.0",
  "description": "Secure code execution engine for AI agents",
  "author": "Illusion47586",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Illusion47586/isol8.git"
  },
  "homepage": "https://github.com/Illusion47586/isol8",
  "bugs": {
    "url": "https://github.com/Illusion47586/isol8/issues"
  },
  "keywords": [
    "sandbox",
    "docker",
    "code-execution",
    "isolation",
    "security",
    "ai-agents",
    "container",
    "runtime"
  ],
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:prod": "turbo run test:prod",
    "lint:check": "turbo run lint:check",
    "lint:fix": "turbo run lint:fix",
    "dev": "bun run --filter @isol8/cli dev",
    "schema": "turbo run schema",
    "docs:dev": "bun run --filter @isol8/docs dev",
    "docs:validate": "bun run --filter @isol8/docs validate",
    "docs:broken-links": "bun run --filter @isol8/docs broken-links"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "turbo": "^2.4.4",
    "typescript": "^5.9.3",
    "ultracite": "^7.2.0"
  },
  "packageManager": "bun@1.2.4"
}
